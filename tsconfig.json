{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "node",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "lib": [
      "ES2020"
    ]
  },
  "include": [
    "src"
  ]
}
